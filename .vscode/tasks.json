{
  "version": "2.0.0",
  "tasks": [
    { // this uses: nightly-x86_64-pc-windows-msvc (from rust-toolchain.toml)
      "label": "cargo build rqcoresrv",
      "type": "shell",
      "command": "cargo build",
      "options": {

        "cwd": "${workspaceFolder}/src/rqcoresrv"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$rustc"]
    },
    {
      "label": "cargo build open_urls_together",
      "type": "shell",
      "command": "cargo build",
      "options": {
        "cwd": "${workspaceFolder}/src/tools/agy/open_urls_together"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$rustc"]
    }
  ]
}
